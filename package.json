{
  "name": "ldclient-js",
  "version": "1.0.0",
  "description": "LaunchDarkly SDK for JavaScript",
  "author": "LaunchDarkly <team@launchdarkly.com>",
  "license": "Apache 2.0",
  "keywords": [
    "launchdarkly",
    "analytics",
    "client"
  ],
  "main": "src",
  "scripts": {
    "build-dev": "browserify src/index.js --standalone LDClient -o dist/ldclient.js",
    "build-min": "browserify src/index.js --standalone LDClient | uglifyjs > dist/ldclient.min.js",
    "build-gzip": "browserify src/index.js --standalone LDClient | uglifyjs | gzip > dist/ldclient.min.js.gz",
    "build": "npm run build-dev && npm run build-min && npm run build-gzip",
    "watch": "watchify src/index.js --debug --verbose --standalone LDClient -o dist/ldclient.js"
  },
  "dependencies": {
    "browserify": "13.0.1",
    "uglify-js": "2.6.3",
    "watchify": "3.7.0"
  }
}
